{
  "name": "aidX",
  "version": "0.2.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "build": "next build",
    "prebuild": "npm run i18n:convert:all",
    "lint": "next lint",
    "optimize-images": "bash ./scripts/optimize-images.sh",

    "i18n:po:code": "ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/extract-po-from-code.ts",
    "i18n:po:json": "node scripts/json-to-pot.js",
    "i18n:po:all": "npm run i18n:po:code && npm run i18n:po:json",

    "i18n:convert:en": "node scripts/po-to-json.js en",
    "i18n:convert:de": "node scripts/po-to-json.js de",
    "i18n:convert:fr": "node scripts/po-to-json.js fr",
    "i18n:convert:all": "npm run i18n:convert:en && npm run i18n:convert:de && npm run i18n:convert:fr",

    "i18n:codemod:dry": "TS_NODE_PROJECT=tsconfig.scripts.json ts-node scripts/i18n-codemod.ts",
    "i18n:codemod:write": "TS_NODE_PROJECT=tsconfig.scripts.json ts-node scripts/i18n-codemod.ts --write",
    "i18n:codemod:one": "TS_NODE_PROJECT=tsconfig.scripts.json ts-node scripts/i18n-codemod.ts --write --include",
    "i18n:mapping:build": "TS_NODE_PROJECT=tsconfig.scripts.json ts-node scripts/i18n-build-mapping.ts"
  },
  "dependencies": {
    "@chakra-ui/next-js": "^2.2.0",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/styled": "^11.11.5",
    "@eslint/config-array": "^0.21.0",
    "@eslint/object-schema": "^2.1.6",
    "@headlessui/react": "^1.7.13",
    "@headlessui/tailwindcss": "^0.1.2",
    "@transifex/native": "^7.1.4",
    "@transifex/react": "^7.1.4",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@types/react-datepicker": "^4.10.0",
    "@types/react-input-mask": "^3.0.2",
    "@types/react-syntax-highlighter": "^15.5.6",
    "@types/scroll-lock": "^2.1.0",
    "core-js": "^3.45.0",
    "encoding": "^0.1.13",
    "framer-motion": "^11.1.7",
    "gettext": "^0.0.1",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "i18next-scanner": "^4.6.0",
    "lru-cache": "^11.1.0",
    "lucide-react": "^0.525.0",
    "next": "14.2.3",
    "rc-slider": "^10.1.1",
    "react": "^18",
    "react-copy-to-clipboard": "^5.1.0",
    "react-datepicker": "^4.10.0",
    "react-dom": "^18",
    "react-hot-toast": "^2.4.0",
    "react-i18next": "^15.6.1",
    "react-input-mask": "^3.0.0-alpha.2",
    "react-responsive": "^9.0.2",
    "react-select": "^5.7.2",
    "react-syntax-highlighter": "^15.5.0",
    "react-textarea-autosize": "^8.4.0",
    "scroll-lock": "^2.1.5",
    "sharp": "^0.34.3",
    "tailwind-merge": "^1.10.0",
    "tailwind-scrollbar": "^3.0.0"
  },
  "devDependencies": {
    "@babel/generator": "^7.28.3",
    "@babel/parser": "^7.28.3",
    "@babel/traverse": "^7.28.3",
    "@babel/types": "^7.28.2",
    "@rvxlab/tailwind-plugin-ios-full-height": "^1.1.0",
    "@transifex/cli": "^7.1.4",
    "@types/change-case": "^0.0.30",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.19.10",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/yargs": "^17.0.33",
    "change-case": "^5.4.4",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.31",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.1",
    "gettext-extractor": "^4.0.1",
    "gettext-parser": "^8.0.0",
    "postcss": "^8",
    "postcss-import": "^15.1.0",
    "tailwindcss": "^3.4.1",
    "ts-morph": "^26.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "yargs": "^18.0.0"
  }
}
