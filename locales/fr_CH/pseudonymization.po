#. key: pseudonymization.status.idle
msgid "Privacy protection inactive"
msgstr "Protection de la vie privée inactive"

#. key: pseudonymization.status.loading
msgid "Loading advanced detection..."
msgstr "Chargement de la détection avancée..."

#. key: pseudonymization.status.loading-progress
msgid "Loading advanced detection ({percent}%)"
msgstr "Chargement de la détection avancée ({pourcentage} %)"

#. key: pseudonymization.status.ready
msgid "Advanced detection active"
msgstr "Détection avancée active"

#. key: pseudonymization.status.degraded
msgid "Basic detection only (pattern-based)"
msgstr "Détection de base uniquement (basée sur des modèles)"

#. key: pseudonymization.labels.pii-secure
msgid "PII detected"
msgstr "PII détecté"

#. key: pseudonymization.labels.pii-uncertain
msgid "PII uncertain"
msgstr "Informations personnelles identifiables incertaines"

#. key: pseudonymization.labels.fachbegriff
msgid "Technical term detected"
msgstr "Terme technique détecté"

#. key: pseudonymization.labels.category-person
msgid "Person"
msgstr "Personne"

#. key: pseudonymization.labels.category-ort
msgid "Location"
msgstr "Emplacement"

#. key: pseudonymization.labels.category-org
msgid "Organization"
msgstr "Organisation"

#. key: pseudonymization.labels.category-datum
msgid "Date"
msgstr "Date"

#. key: pseudonymization.labels.category-email
msgid "Email"
msgstr "Courriel"

#. key: pseudonymization.labels.category-telefon
msgid "Phone"
msgstr "Téléphone"

#. key: pseudonymization.labels.category-sozialvers
msgid "Social security number"
msgstr "numéro de sécurité sociale"

#. key: pseudonymization.labels.category-iban
msgid "IBAN"
msgstr "IBAN"

#. key: pseudonymization.labels.category-steuernr
msgid "Tax ID"
msgstr "numéro d'identification fiscale"

#. key: pseudonymization.labels.category-plz
msgid "Postal code"
msgstr "Code postal"

#. key: pseudonymization.labels.category-diagnose
msgid "Diagnosis"
msgstr "Diagnostic"

#. key: pseudonymization.labels.category-medikament
msgid "Medication"
msgstr "Médicaments"

#. key: pseudonymization.labels.category-therapie
msgid "Therapy"
msgstr "Thérapie"

#. key: pseudonymization.review.title
msgid "Review detected information"
msgstr "Vérifier les informations détectées"

#. key: pseudonymization.review.description
msgid ""
"The following personal information was detected in your message. Confirmed "
"items will be replaced with tokens before sending."
msgstr ""
"Les informations personnelles suivantes ont été détectées dans votre "
"message. Les éléments confirmés seront remplacés par des jetons avant "
"l'envoi."

#. key: pseudonymization.buttons.accept-all
msgid "Accept all"
msgstr "Accepter tout"

#. key: pseudonymization.buttons.send
msgid "Send"
msgstr "Envoyer"

#. key: pseudonymization.buttons.cancel
msgid "Cancel"
msgstr "Annuler"

#. key: pseudonymization.review.source-regex
msgid "Pattern"
msgstr "Motif"

#. key: pseudonymization.review.source-ner
msgid "AI detection"
msgstr "Détection par IA"

#. key: pseudonymization.review.source-dictionary
msgid "Dictionary"
msgstr "Dictionnaire"

#. key: pseudonymization.review.no-detections
msgid "No personal information detected."
msgstr "Aucune information personnelle détectée."

#. key: pseudonymization.buttons.reject-all
msgid "Reject all"
msgstr "Tout rejeter"

#. key: pseudonymization.file.error-type
msgid "Unsupported file type. Please upload a .txt, .pdf or .docx file."
msgstr ""
"Type de fichier non pris en charge. Veuillez télécharger un "
"fichier .txt, .pdf ou .docx."

#. key: pseudonymization.file.error-size
msgid ""
"File is too large. Maximum size: 10 MB for text files, 50 MB for PDFs and "
"Word documents."
msgstr ""
"Le fichier est trop volumineux. Taille maximale : 10 Mo pour les fichiers "
"texte, 50 Mo pour les fichiers PDF et les documents Word."

#. key: pseudonymization.file.error-empty
msgid "The file is empty. Please select a file with content."
msgstr ""
"Le fichier est vide. Veuillez sélectionner un fichier contenant du contenu."

#. key: pseudonymization.file.error-no-text
msgid "No text could be extracted from this file."
msgstr "Aucun texte n'a pu être extrait de ce fichier."

#. key: pseudonymization.file.error-extraction
msgid "Could not read the file. Please try a different file."
msgstr "Impossible de lire le fichier. Veuillez essayer un autre fichier."

#. key: pseudonymization.file.error-upload
msgid "Upload failed. Please check your connection and try again."
msgstr ""
"Échec du téléchargement. Veuillez vérifier votre connexion et réessayer."

#. key: pseudonymization.file.phase-validating
msgid "Validating file…"
msgstr "Validation du fichier…"

#. key: pseudonymization.file.phase-extracting
msgid "Extracting text…"
msgstr "Extraction du texte…"

#. key: pseudonymization.file.phase-detecting
msgid "Detecting personal information…"
msgstr "Détection des informations personnelles…"

#. key: pseudonymization.file.phase-uploading
msgid "Uploading…"
msgstr "Téléchargement en cours…"

#. key: pseudonymization.file.upload-button
msgid "Upload document"
msgstr "Télécharger le document"

#. key: pseudonymization.status.processing
msgid "Processing…"
msgstr "Traitement en cours…"

#. key: pseudonymization.review.attach-button
#. CTA in file mode — replaces "Send"
msgid "Pseudonymise & Attach"
msgstr "Pseudonymiser et joindre"

#. key: pseudonymization.review.legend-secure
#. Legend entry for regex or NER ≥ 0.85
msgid "Securely detected"
msgstr "Détecté de manière sécurisée"

#. key: pseudonymization.review.legend-uncertain
#. Legend entry for NER 0.60–0.85
msgid "Uncertain detection"
msgstr "Détection incertaine"

#. key: pseudonymization.review.legend-dictionary
#. Legend entry for dictionary (medical/therapy terms)
msgid "Technical term"
msgstr "Terme technique"

#. key: pseudonymization.review.detection-singular
#. Count label when exactly 1 detection
msgid "1 detection"
msgstr "1 détection"

#. key: pseudonymization.review.detection-plural
#. Count label when 2+ detections; {{count}} is replaced with the number
msgid "{{count}} detections"
msgstr "{{count}} détections"

#. key: pseudonymization.review.hint-select-one
#. Shown when all detections are rejected (Send disabled)
msgid "Please accept at least one detection to proceed."
msgstr "Veuillez accepter au moins une détection pour continuer."

#. key: pseudonymization.review.page-indicator
#. Pagination indicator inside document preview; {{current}} and {{total}} are replaced
msgid "Page {{current}} of {{total}}"
msgstr "Page {{current}} sur {{total}}"

#. key: pseudonymization.review.prev-page
#. Button label / aria-label for previous page
msgid "Previous"
msgstr "Précédent"

#. key: pseudonymization.review.next-page
#. Button label / aria-label for next page
msgid "Next"
msgstr "Suivant"

#. key: pseudonymization.review.click-to-toggle
#. Interaction hint shown in the document preview legend
msgid "Click to accept or reject"
msgstr "Cliquez pour accepter ou refuser"

#. key: pseudonymization.attachment.preview
#. aria-label for the AttachmentChip button (opens read-only preview)
msgid "Open document preview"
msgstr "Ouvrir l'aperçu du document"

#. key: pseudonymization.attachment.remove
#. aria-label for the ✕ button on the AttachmentChip
msgid "Remove attachment"
msgstr "Supprimer la pièce jointe"

#. key: pseudonymization.attachment.modal-title
#. Title of the ModalDocumentPreview
msgid "Attached document"
msgstr "Document joint"

#. key: pseudonymization.attachment.show-original
#. Toggle button label: reveals original PII (owner-only, client-side)
msgid "Show original"
msgstr "Afficher l'original"

#. key: pseudonymization.attachment.show-pseudonymized
#. Toggle button label: hides original, shows pseudonymized tokens
msgid "Show pseudonymised"
msgstr "Afficher pseudonymisé"

#. key: pseudonymization.attachment.export-word
#. Export button label for Word (.docx) download in ModalDocumentPreview
msgid "Export Word"
msgstr "Exporter Word"

#. key: pseudonymization.attachment.export-pdf
#. Export button label for PDF download in ModalDocumentPreview
msgid "Export PDF"
msgstr "Exporter au format PDF"

#. key: pseudonymization.review.mark-as-pii
#. Floating popup button label for manual PII marking (SPEC-007a §4.5)
msgid "Mark as PII"
msgstr "Marquer comme PII"

#. key: pseudonymization.review.manual-label
#. Badge label in review list for manually marked items (source: 'manual')
msgid "Manually marked"
msgstr "Marqué manuellement"

#. key: pseudonymization.review.manual-hint
#. Hint shown below document preview to invite manual marking
msgid "Select text to mark it as PII"
msgstr "Sélectionnez le texte pour le marquer comme PII."
