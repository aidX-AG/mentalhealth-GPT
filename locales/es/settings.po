#. key: settings.edit-profile.title
msgid "Edit profile"
msgstr "Editar perfil"

#. key: settings.edit-profile.avatar.label
msgid "Profile picture"
msgstr ""

#. key: settings.edit-profile.avatar.alt
msgid "User profile picture"
msgstr ""

#. key: settings.edit-profile.avatar.upload
msgid "Upload new image"
msgstr "Subir nueva imagen"

#. key: settings.edit-profile.avatar.encrypting
msgid "Encrypting…"
msgstr "Cifrando…"

#. key: settings.edit-profile.avatar.hint.resolution
msgid "At least 800x800 px recommended."
msgstr "Se recomienda un tamaño mínimo de 800 x 800 píxeles."

#. key: settings.edit-profile.avatar.hint.types
msgid "JPG, PNG, GIF or WEBP allowed."
msgstr "Se admiten formatos JPG, PNG, GIF o WEBP."

#. key: settings.edit-profile.avatar.error.type
msgid "Please upload a JPG, PNG, GIF or WEBP image."
msgstr "Por favor, sube una imagen en formato JPG, PNG, GIF o WEBP."

#. key: settings.edit-profile.avatar.error.size
msgid "The image is too large. Please choose a file up to 5 MB."
msgstr "La imagen es demasiado grande. Elija un archivo de hasta 5 MB."

#. key: settings.edit-profile.avatar.error.encrypt_failed
msgid "Encryption failed. Please try again."
msgstr "El cifrado ha fallado. Inténtalo de nuevo."

#. key: settings.edit-profile.name.label
msgid "Name"
msgstr "Nombre"

#. key: settings.edit-profile.name.placeholder
msgid "Username"
msgstr "Nombre de usuario"

#. key: settings.edit-profile.location.label
msgid "Location"
msgstr "Ubicación"

#. key: settings.edit-profile.location.placeholder
msgid "My private mentalhealthGPT workspace"
msgstr "Mi espacio de trabajo privado mentalhealthGPT"

#. key: settings.edit-profile.bio.label
msgid "Bio"
msgstr "Biografía"

#. key: settings.edit-profile.bio.placeholder
msgid "Short bio"
msgstr "Breve biografía"

#. key: settings.edit-profile.save
msgid "Save changes"
msgstr "Guardar cambios"
